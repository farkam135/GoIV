        "dodge": {
            "label": "Dodge Strategy",

            "DODGE_100": {
                "label": "Perfect Dodging",
                "description": "Tries to dodge 100% of the time. This dodge strategy will almost never miss a dodge. You may still get hit in certain unavoidable situations such as when using a special move"
            },
            "DODGE_REACTION_TIME": {
                "label": "Realistic Dodging",
                "description": "Attempts to simulate a realistic human dodger. Dodges vary from 50% success rate for a fast 1 second move to 90% success rate for a slow 3.5s move. In addition, if the attacker is using either super effective or not effective attacks, there is an additional 25% penalty to dodging, dropping the success rate to 37.5% to 67.5%. Finally a small bonus for faster attacker moves is added based on how much faster the move is than the dodge flash."
            },
            "DODGE_REACTION_TIME2": {
                "label": "Realistic Dodging Pro",
                "description": "Attempts to simulate a realistic experienced human dodger. Dodges vary from 70% success rate for a fast 1 second move to 90% success rate for a slow 3.5s move. In addition, if the attacker is using either super effective or not effective attacks, there is an additional 10% penalty to dodging, dropping the success rate to 63.0% to 81.0%. Finally a small bonus for faster attack moves is added based on how much faster the move is than the dodge flash."
            },
            "DODGE_25": {
                "label": "25% Dodging",
                "description": "Chooses to dodge 25% of the time"
            },
            "DODGE_50": {
                "label": "50% Dodging",
                "description": "Chooses to dodge 50% of the time"
            },
            "DODGE_75": {
                "label": "75% Dodging",
                "description": "Chooses to dodge 75% of the time"
            },
            "DODGE_90": {
                "label": "90% Dodging",
                "description": "Chooses to dodge 90% of the time"
            }
        },
        "shield": {
            "label": "Shield Strategy",
            "SHIELD_0": {
                "label": "Never Shield",
                "description": "Never tries to shield"
            },
            "SHIELD_1": {
                "label": "Shield Once",
                "description": "Shields the first special move it can"
            },
            "SHIELD_2": {
                "label": "Shield Twice",
                "description": "Shields the first two special moves it can"
            }
        },
        "defense": {
            "DEFENSE": {
                "label": "Expected Defender",
                "description": "Statistically valid expected result. Attacks come every 2 seconds and special attacks are always 1 attack late."
            },
            "DEFENSE_LUCKY": {
                "label": "Lucky Defender",
                "description": "About as bad as it can get. Attacks come every 1.95s and special attacks are as early as possible"
            },
            "DEFENSE_RANDOM": {
                "label": "Random",
                "description": "Full random simulation! Attacks come between 1.5 and 2.5s apart. Special attacks are used 50% of the time if the defender has enough energy"
            },
            "DEFENSE_RANDOM_MC_FAST": {
                "label": "Monte Carlo - Bronze",
                "description": "A more accurate result. 10 random simulations are run and averaged together to make one overall result. The median result based on overall rating is used for combat logs as an example. Most useful in combination with non-perfect dodge strategies."
            },
            "DEFENSE_RANDOM_MC_MID": {
                "label": "Monte Carlo - Silver",
                "description": "A highly accurate result. 50 random simulations are run and averaged together to make one overall result. The median result based on overall rating is used for combat logs as an example. Most useful in combination with non-perfect dodge strategies."
            },
            "DEFENSE_RANDOM_MC": {
                "label": "Monte Carlo - Gold",
                "description": "The most accurate result. 200 random simulations are run and averaged together to make one overall result. The median result based on overall rating is used for combat logs as an example. Most useful in combination with non-perfect dodge strategies."
            },
            "DEFENSE_RANDOM_MC_EX": {
                "label": "Monte Carlo - Platinum",
                "description": "A special benefit for Fanatics on Patreon. An excessively accurate result. 500 random simulations are run and combined together to make one overall result based on your aggregation choice. Most useful when looking at percentiles"
            },
            "DEFENSE_PATTERN": {
                "label": "Defense Pattern - RNG",
                "description": "An extremely predictable random number generator is used for this single simulation. This is used internally by Pokebattler for optimizing Monte Carlo simulations with random dodging"
            },
            "DEFENSE_PATTERN2": {
                "label": "Defense Pattern",
                "description": "A pattern of delays and attacks is used for this single simulation. This is used internally by Pokebattler for optimizing Monte-Carlo simulations without dodging"
            }
